<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
		version="2.5">
	<!-- Step1 -->
    <servlet>
        <servlet-name>LoginDisplayServlet</servlet-name>
        <servlet-class>app.servlet.LoginDisplayServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginProcessServlet</servlet-name>
        <servlet-class>app.servlet.LoginProcessServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListDisplayServlet</servlet-name>
        <servlet-class>app.servlet.ListDisplayServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginDisplayServlet</servlet-name>
        <url-pattern>/login.action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginProcessServlet</servlet-name>
        <url-pattern>/loginProcess.action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListDisplayServlet</servlet-name>
        <url-pattern>/list.action</url-pattern>
    </servlet-mapping>
    <!-- Other -->
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
